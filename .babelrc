{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-react", {"runtime": "automatic"}]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "version": "legacy" }],
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-transform-object-rest-spread"
  ]
}
